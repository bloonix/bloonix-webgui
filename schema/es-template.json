{
    "template": "bloonix-*",
    "settings" : {
        "index.number_of_shards" : 4,
        "index.number_of_replicas" : 1
    },
    "mappings": {
        "stats" : {
            "dynamic_templates" : [
                {
                    "match": {
                        "match": "*",
                        "match_mapping_type" : "*",
                        "mapping" : {
                            "type": "string",
                            "index" : "no"
                        }
                    }
                }
            ],
            "properties" : {
                "time" : { "type" : "date", "index" : "not_analyzed" },
                "host_id" : { "type" : "long", "index" : "not_analyzed" },
                "service_id" : { "type" : "long", "index" : "not_analyzed" },
                "plugin" : { "type" : "string", "index" : "not_analyzed" },
                "subkey" : { "type" : "string", "index" : "not_analyzed" },
                "data" : {
                    "type" : "object",
                    "enabled" : false
                }
            }
        },
        "event" : {
            "dynamic_templates" : [
                {
                    "match": {
                        "match": "*",
                        "match_mapping_type" : "*",
                        "mapping" : {
                            "type": "string",
                            "index" : "no"
                        }
                    }
                }
            ],
            "properties" : {
                "time" : { "type" : "date", "index" : "not_analyzed" },
                "host_id" : { "type" : "long", "index" : "not_analyzed" },
                "service_id" : { "type" : "long", "index" : "not_analyzed" },
                "last_status" : { "type" : "string", "index" : "not_analyzed" },
                "status" : { "type" : "string", "index" : "not_analyzed" },
                "message" : { "type" : "string" },
                "attempts" : { "type" : "string", "index" : "not_analyzed" },
                "tags" : { "type" : "string" },
                "duration" : { "type" : "long", "index" : "not_analyzed" },
                "result" : {
                    "type" : "object",
                    "enabled" : false
                },
                "debug" : {
                    "type" : "object",
                    "enabled" : false
                }
            }
        },
        "results" : {
            "dynamic_templates" : [
                {
                    "match": {
                        "match": "*",
                        "match_mapping_type" : "*",
                        "mapping" : {
                            "type": "string",
                            "index" : "no"
                        }
                    }
                }
            ],
            "properties" : {
                "time" : { "type" : "date", "index" : "not_analyzed" },
                "host_id" : { "type" : "long", "index" : "not_analyzed" },
                "service_id" : { "type" : "long", "index" : "not_analyzed" },
                "status" : { "type" : "string", "index" : "not_analyzed" },
                "message" : { "type" : "string" },
                "attempts" : { "type" : "string", "index" : "not_analyzed" },
                "data" : {
                    "type" : "object",
                    "enabled" : false
                }
            }
        }
    }
}
